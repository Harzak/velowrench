<UserControl x:Class="velowrench.Views.ChainLengthCalculatorView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:res="clr-namespace:velowrench.Resources"
             xmlns:views="clr-namespace:velowrench.Views"
             xmlns:vm="clr-namespace:velowrench.ViewModels"
             x:DataType="vm:HomeViewModel"
             Design.Height="640"
             Design.Width="360">

    <Grid ColumnDefinitions="*" RowDefinitions="Auto, Auto, Auto">

        <views:RoutableViewHeaderControl Grid.Row="0" Grid.Column="0" />

        <TextBlock Grid.Row="1" Grid.Column="0"
                   Margin="100,50"
                   Text="ChainLengthCalculatorView" />

    </Grid>
</UserControl>
